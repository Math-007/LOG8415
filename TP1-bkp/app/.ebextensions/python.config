option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: app:app
    NumProcesses: 1
    NumThreads: 1
  aws:autoscaling:asg:
    Availability Zones: Any
    Cooldown: '720'
    MaxSize: '4'
    MinSize: '4'
    EnableCapacityRebalancing: true
  aws:elasticbeanstalk:command:
    DeploymentPolicy: AllAtOnce